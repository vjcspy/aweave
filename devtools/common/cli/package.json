{
  "name": "@aweave/cli",
  "version": "0.1.0",
  "private": true,
  "bin": {
    "aw": "./bin/run.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "bin/dev.js",
    "link-global": "pnpm link --global",
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "oclif manifest"
  },
  "oclif": {
    "bin": "aw",
    "dirname": "aweave",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "plugins": [
      "@aweave/cli-plugin-debate",
      "@aweave/cli-plugin-docs",
      "@aweave/cli-plugin-dashboard",
      "@aweave/cli-plugin-relay",
      "@aweave/cli-plugin-demo-workflow"
    ]
  },
  "dependencies": {
    "@aweave/cli-plugin-dashboard": "workspace:*",
    "@aweave/cli-plugin-debate": "workspace:*",
    "@aweave/cli-plugin-demo-workflow": "workspace:*",
    "@aweave/cli-plugin-docs": "workspace:*",
    "@aweave/cli-plugin-relay": "workspace:*",
    "@aweave/cli-shared": "workspace:*",
    "@oclif/core": "catalog:",
    "@oclif/plugin-help": "^6.2.27"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "oclif": "^4.17.41",
    "ts-node": "catalog:",
    "typescript": "catalog:"
  }
}
