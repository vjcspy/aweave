{
  "name": "@hod/aweave",
  "version": "0.1.24",
  "bin": {
    "aw": "./bin/run.js"
  },
  "files": [
    "bin",
    "dist",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "tsc && tsc-alias -p tsconfig.json",
    "oclif:manifest": "oclif manifest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "dev": "bin/dev.js",
    "link-global": "pnpm link --global"
  },
  "oclif": {
    "bin": "aw",
    "dirname": "aweave",
    "commands": "./dist/commands",
    "topicSeparator": " ",
    "plugins": [
      "@hod/aweave-plugin-debate",
      "@hod/aweave-plugin-docs",
      "@hod/aweave-plugin-dashboard",
      "@hod/aweave-plugin-relay",
      "@hod/aweave-plugin-demo-workflow",
      "@hod/aweave-plugin-config",
      "@hod/aweave-plugin-server",
      "@hod/aweave-plugin-test",
      "@hod/aweave-plugin-workspace"
    ]
  },
  "dependencies": {
    "@hod/aweave-plugin-config": "workspace:*",
    "@hod/aweave-plugin-dashboard": "workspace:*",
    "@hod/aweave-plugin-debate": "workspace:*",
    "@hod/aweave-plugin-demo-workflow": "workspace:*",
    "@hod/aweave-plugin-docs": "workspace:*",
    "@hod/aweave-plugin-relay": "workspace:*",
    "@hod/aweave-plugin-server": "workspace:*",
    "@hod/aweave-plugin-test": "workspace:*",
    "@hod/aweave-cli-shared": "workspace:*",
    "@oclif/core": "catalog:",
    "@oclif/plugin-help": "^6.2.27",
    "@hod/aweave-server": "workspace:*",
    "@hod/aweave-plugin-workspace": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "oclif": "^4.17.41",
    "ts-node": "catalog:",
    "tsc-alias": "catalog:",
    "typescript": "catalog:"
  }
}
